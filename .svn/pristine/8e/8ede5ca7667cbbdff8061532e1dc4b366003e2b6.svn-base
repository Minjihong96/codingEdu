package web.com.springweb.jihong.jh_vo;

import java.util.Date;

import org.springframework.format.annotation.DateTimeFormat;

public class ClassRecord {
	private int recordId;
	private int memPrid;
	@DateTimeFormat(pattern = "yyyy-MM-dd")
	private Date classDate;
	private String classContents;
	private String subject;
	private int remainClass;
	private int payment;
	private String etc;
	public ClassRecord() {
		super();
		// TODO Auto-generated constructor stub
	}
	public ClassRecord(int recordId, int memPrid, Date classDate, String classContents, String subject,
			int remainClass, int payment, String etc) {
		super();
		this.recordId = recordId;
		this.memPrid = memPrid;
		this.classDate = classDate;
		this.classContents = classContents;
		this.subject = subject;
		this.remainClass = remainClass;
		this.payment = payment;
		this.etc = etc;
	}
	public int getRecordId() {
		return recordId;
	}
	public void setRecordId(int recordId) {
		this.recordId = recordId;
	}
	public int getMemPrid() {
		return memPrid;
	}
	public void setMemPrid(int memPrid) {
		this.memPrid = memPrid;
	}
	public Date getClassDate() {
		return classDate;
	}
	public void setClassDate(Date classDate) {
		this.classDate = classDate;
	}
	public String getClassContents() {
		return classContents;
	}
	public void setClassContents(String classContents) {
		this.classContents = classContents;
	}
	public String getSubject() {
		return subject;
	}
	public void setSubject(String subject) {
		this.subject = subject;
	}
	public int getRemainClass() {
		return remainClass;
	}
	public void setRemainClass(int remainClass) {
		this.remainClass = remainClass;
	}
	public int getPayment() {
		return payment;
	}
	public void setPayment(int payment) {
		this.payment = payment;
	}
	public String getEtc() {
		return etc;
	}
	public void setEtc(String etc) {
		this.etc = etc;
	}
	
	
}
